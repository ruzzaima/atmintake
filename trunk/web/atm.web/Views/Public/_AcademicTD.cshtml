
<div class="row-fluid">
    <div data-bind="foreach: applicant.ApplicantEducations">
        <input type="hidden" data-bind="value: $data.ApplicantEduId" />
        <!-- ko if : $data.HighEduLevelCd() === '15' -->
        <div class="col-md-4">
            <div class="panel panel-default tile">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span data-bind="text: $data.HighEduLevel"></span>
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Tahun</label>
                                <input type="text" class="form-control" data-bind="value: $data.ConfermentYr, valueUpdate: 'afterkeydown'" maxlength="4" />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Keputusan</label>
                        <input type="text" class="form-control" data-bind="value: $data.OverallGrade, valueUpdate: 'afterkeydown'" />
                    </div>
                    <div data-bind="foreach: $data.ApplicantEduSubjectCollection">
                        <div class="row">
                            <div class="m-t-10 form-inline">
                                <label class="control-label" data-bind="text: $data.Subject"></label>
                                <select class="form-control col-md-2 atm-select pull-right" data-bind="options: $root.grades, optionsText: 'Name', optionsValue: 'Code', optionsCaption: 'Sila Pilih', value: $data.Grade"></select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ko-->
        <!-- ko if : $data.HighEduLevelCd() === '14'-->
        <div class="col-md-4">
            <div class="panel panel-default tile">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span data-bind="text: $data.HighEduLevel"></span>
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Tahun<span class="red">*</span></label>
                                <input type="text" class="form-control validate[required]" data-bind="value: $data.ConfermentYr, valueUpdate: 'afterkeydown'" maxlength="4" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Keputusan<span class="red">*</span></label>
                                <input type="text" class="form-control validate[required]" data-bind="value: $data.OverallGrade, valueUpdate: 'afterkeydown'" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12"><i>Contoh : (4A 3B 2C)</i></div>
                    </div>
                    <div data-bind="foreach: $data.ApplicantEduSubjectCollection">
                        <div class="row">
                            <div class="m-t-10">
                                <div class="col-md-8">
                                    <!-- ko if : $index() + 1 <= 10 -->
                                    <!-- ko if : $data.SubjectCd() === 1 || $data.SubjectCd() === 2 || $data.SubjectCd() === 3 -->
                                    <label class="control-label" data-bind="text: $data.Subject"></label><span class="red">*</span>
                                    <!--/ko-->
                                    <!-- ko ifnot : $data.SubjectCd() === 1 || $data.SubjectCd() === 2 || $data.SubjectCd() === 3 -->
                                    <label class="control-label" data-bind="text: $data.Subject"></label>
                                    <!--/ko-->
                                    <!--/ko-->
                                    <!-- ko if : $index() + 1 >= 11-->
                                    <select class="form-control atm-select " data-bind="options: $root.subjects, optionsText: 'Name', optionsValue: 'Code', optionsCaption: 'Sila Pilih', valueAllowUnset: true, value: $data.SubjectCd"></select>
                                    <!--/ko-->
                                    <input type="hidden" data-bind="attr : {id: $data.SubjectCd() + '_' + ($index() + 1)}, value: $data.SubjectCd" />
                                </div>
                                <div class="col-md-4">
                                    <!-- ko if : $data.SubjectCd() === 1 || $data.SubjectCd() === 2 || $data.SubjectCd() === 3 -->
                                    <select class="form-control atm-select input-sm pull-right validate[required]" data-bind="options: $root.grades, optionsText: 'Name', optionsValue: 'Code', optionsCaption: 'Sila Pilih', valueAllowUnset: true, value: $data.GradeCd"></select>
                                    <!--/ko-->
                                    <!-- ko ifnot : $data.SubjectCd() === 1 || $data.SubjectCd() === 2 || $data.SubjectCd() === 3 -->
                                    <select class="form-control atm-select input-sm pull-right" data-bind="options: $root.grades, optionsText: 'Name', optionsValue: 'Code', optionsCaption: 'Sila Pilih', valueAllowUnset: true, value: $data.GradeCd"></select>
                                    <!--/ko-->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ko-->
        <!-- ko if : $data.HighEduLevelCd() === '13'-->
        <div class="col-md-4">
            <div class="panel panel-default tile">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span data-bind="text: $data.HighEduLevel"></span>
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Tahun</label>
                                <input type="text" class="form-control" data-bind="value: $data.ConfermentYr, valueUpdate: 'afterkeydown'" maxlength="4" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Keputusan</label>
                                <input type="text" class="form-control" data-bind="value: $data.OverallGrade, valueUpdate: 'afterkeydown'" />
                                
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12"><i>Contoh : 3.67, 3 prinsipal</i></div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ko-->
        <!-- ko if : $data.HighEduLevelCd() === '25'-->
        <div class="col-md-4">
            <div class="panel panel-default tile">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span data-bind="text: $data.HighEduLevel"></span>
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Tahun</label>
                                <input type="text" class="form-control eduyear" data-bind="value: $data.ConfermentYr, valueUpdate: 'afterkeydown'" maxlength="4" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Keputusan</label>
                                <input type="text" class="form-control" data-bind="value: $data.OverallGrade, valueUpdate: 'afterkeydown'" />
                                
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12"><i>Contoh : Mumtaz, Jayyid Jiddan, Maqbul</i></div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ko-->
        <!-- ko if : $data.HighEduLevelCd() === '26'-->
        <div class="col-md-4">
            <div class="panel panel-default tile">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span data-bind="text: $data.HighEduLevel"></span>
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Tahun</label>
                                <input type="text" class="form-control" data-bind="value: $data.ConfermentYr, valueUpdate: 'afterkeydown'" maxlength="4" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label">Bidang</label>
                                <input type="text" class="form-control" data-bind="value: $data.EduCertTitle, valueUpdate: 'afterkeydown'" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <label class="control-label col-md-12">Tahap</label>
                            <div class="col-md-4">
                                <select class="form-control atm-select" data-bind="value: $data.SKMLevel">
                                    <option value="">Sila Pilih</option>
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="3">3</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label">Pusat Pengajian</label>
                                <input type="text" class="form-control" data-bind="value: $data.InstitutionName, valueUpdate: 'afterkeydown'" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ko-->
        <!-- ko if : $data.HighEduLevelCd() === '11'-->
        <div class="col-md-4">
            <div class="panel panel-default tile">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span data-bind="text: $data.HighEduLevel"></span>
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Tahun</label>
                                <input type="text" class="form-control" data-bind="value: $data.ConfermentYr, valueUpdate: 'afterkeydown'" maxlength="4" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label">Keputusan(CGPA)</label>
                                <input type="text" class="form-control" data-bind="value: $data.OverallGrade, valueUpdate: 'afterkeydown'" />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Bidang</label>
                        <select class="form-control atm-select " data-bind="options: $root.bidang, optionsText: 'Name', optionsValue: 'Code', optionsCaption: 'Sila Pilih',valueAllowUnset: true, value: $data.EduCertTitle"></select>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Pusat Pengajian</label>
                        <select class="form-control atm-select" data-bind="options: $root.institutions, optionsText: 'Name', optionsValue: 'Code', optionsCaption: 'Sila Pilih',valueAllowUnset: true, value: $data.InstCd"></select>
                    </div>
                </div>
            </div>
        </div>
        <!--/ko-->
    </div>
</div>
