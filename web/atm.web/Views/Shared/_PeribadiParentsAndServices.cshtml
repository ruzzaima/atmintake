<div class="col-md-4">
    <div class="panel panel-default tile">
        <div class="panel-heading"></div>
        <div class="panel-body">
            <div class="row m-b-5">
                <p>Pernahkah anda berkhidmat di ATM?</p>
                <label class="radio-inline">
                    <input type="radio" class="radio" name="servicetab_exatm" value="Y" /> Ya
                </label>
                <label class="radio-inline">
                    <input type="radio" class="radio" name="servicetab_exatm" value="N" /> Tidak
                </label>
            </div>
            <div class="row" data-bind="visible: applicant.ArmyServiceInd()">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Tempoh Perkhidmatan</label>
                        <div class="input-group">
                            <input type="text" class="form-control input-sm" data-bind="value: applicant.ArmyServiceYrOfServ, valueUpdate: 'afterkeydown'" maxlength="2">
                            <span class="input-group-addon">tahun</span>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="form-group">
                        <label class="control-label">Sebab anda berhenti dari ATM</label>
                        <input type="text" class="form-control input-sm" data-bind="value: applicant.ArmyServiceResignRemark, valueUpdate: 'afterkeydown'">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        <label class="control-label">Pekerjaan Sekarang</label>
                        <input type="text" class="form-control input-sm" data-bind="value: applicant.CurrentOccupation, valueUpdate: 'afterkeydown'">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        <label class="control-label">Nama Tempat Kerja</label>
                        <input type="text" class="form-control input-sm" data-bind="value: applicant.CurrentOrganisation, valueUpdate: 'afterkeydown'">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group">
                    <label class="control-label col-md-12">Gaji</label>
                    <div class="col-md-6">
                        <div class="input-group">
                            <span class="input-group-addon">RM</span>
                            <input type="text" class="form-control input-sm money" data-bind="value: applicant.CurrentSalary, valueUpdate: 'afterkeydown'" maxlength="15">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
