@{
    ViewBag.Title = "Daftar";
}

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                Daftar (Awam)
            </div>
            <div class="panel-body">
                <form class="form-horizontal">
                    <div class="col-md-12">
                        <label>Nama Penuh</label>
                        <input type="text" class="form-control" />
                    </div>
                    <div class="col-md-12">
                        <label>No Kad Pengenalan</label>
                        <input type="text" class="form-control" />
                    </div>
                    <div class="col-md-12">
                        <label>Email</label>
                        <input type="text" class="form-control" />
                    </div>
                    <div class="col-md-12">
                        <label>Email Alternatif</label>
                        <input type="text" class="form-control" />
                    </div>
                    <div class="col-md-12 margin-tb-5">
                        <input type="button" class="btn btn-primary" value="Hantar"/><i style="padding-left: 5px;">Kata laluan akan diemelkan kepada anda.</i>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

